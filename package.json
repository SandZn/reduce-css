{
  "name": "reduce-css",
  "version": "1.0.3",
  "description": "Pack CSS into multiple bundles based on postcss",
  "main": "index.js",
  "scripts": {
    "test": "npm run lint && task-tape test/*.js | tap-summary",
    "lint": "eslint *.js lib/**/*.js test/*.js bin/*.js",
    "coverage": "istanbul cover -i 'lib/**/*.js' -i '*.js' --print both task-tape -- test/*.js",
    "check-coverage": "istanbul check-coverage --statements 90 --functions 90 --branches 85 --lines 90",
    "upload-coverage": "cat ./coverage/lcov.info | coveralls",
    "cover": "npm run coverage && npm run check-coverage && npm run upload-coverage"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zoubin/reduce-css.git"
  },
  "keywords": [
    "postcss",
    "browserify",
    "css",
    "reduce",
    "reduce-css"
  ],
  "author": "zoubin",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/zoubin/reduce-css/issues"
  },
  "homepage": "https://github.com/zoubin/reduce-css#readme",
  "dependencies": {
    "callback-sequence": "^1.2.1",
    "depsify": "^1.0.0",
    "factor-vinylify": "^3.0.0",
    "globby": "^4.0.0",
    "lazypipe": "^1.0.1",
    "postcss": "^5.0.5",
    "postcss-custom-url": "^3.1.1",
    "run-callback": "^2.1.0",
    "stream-combiner2": "^1.1.1",
    "through2": "^2.0.0",
    "util-mix": "^3.0.1",
    "vinyl": "^1.1.0",
    "vinyl-fs": "^2.2.1",
    "watchify": "^3.6.0"
  },
  "devDependencies": {
    "compare-directory": "^1.0.1",
    "coveralls": "^2.11.4",
    "del": "^2.0.2",
    "eslint": "^1.10.1",
    "gulp": "^3.9.0",
    "istanbul": "^0.4.0",
    "mkdirp": "^0.5.1",
    "postcss-advanced-variables": "^1.2.2",
    "postcss-import": "^7.1.0",
    "reduce-css-postcss": "^1.0.5",
    "tap-summary": "^1.0.0",
    "tape": "^4.2.0",
    "task-tape": "^1.0.0"
  }
}
